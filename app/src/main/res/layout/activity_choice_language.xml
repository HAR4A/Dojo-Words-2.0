<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".choice_language">

    <Button
        android:id="@+id/button3"
        android:layout_width="292dp"
        android:layout_height="62dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="314dp"
        android:layout_marginEnd="51dp"
        android:backgroundTint="#FFFFFF"
        android:drawableRight="@drawable/rectangle_5"
        android:fontFamily="@font/manrope_extrabold"
        android:onClick="showNotAvailableDialog"
        android:paddingLeft="20dp"
        android:text="ФРАНЦУЗСКИЙ"
        android:textAlignment="textStart"
        android:textColor="#5A30F2"
        android:textSize="16dp"
        app:cornerRadius="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:strokeColor="#5A30F2"
        app:strokeWidth="2dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="292dp"
        android:layout_height="62dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="392dp"
        android:layout_marginEnd="51dp"
        android:backgroundTint="#FFFFFF"
        android:drawableRight="@drawable/rectangle_6"
        android:fontFamily="@font/manrope_extrabold"
        android:onClick="showNotAvailableDialog"
        android:paddingLeft="20dp"
        android:text="НЕМЕЦКИЙ"
        android:textAlignment="textStart"
        android:textColor="#5A30F2"
        android:textSize="16dp"
        app:cornerRadius="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:strokeColor="#5A30F2"
        app:strokeWidth="2dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="292dp"
        android:layout_height="62dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="470dp"
        android:layout_marginEnd="51dp"
        android:backgroundTint="#FFFFFF"
        android:drawableRight="@drawable/rectangle_7"
        android:fontFamily="@font/manrope_extrabold"
        android:onClick="showNotAvailableDialog"
        android:paddingLeft="20dp"
        android:text="ИСПАНСКИЙ"
        android:textAlignment="textStart"
        android:textColor="#5A30F2"
        android:textSize="16dp"
        app:cornerRadius="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:strokeColor="#5A30F2"
        app:strokeWidth="2dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="292dp"
        android:layout_height="62dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="236dp"
        android:layout_marginEnd="51dp"
        android:backgroundTint="#FFFFFF"
        android:drawableRight="@drawable/rectangle_4"
        android:fontFamily="@font/manrope_extrabold"
        android:onClick="showNotAvailableDialog"
        android:paddingLeft="20dp"
        android:text="ЧЕШСКИЙ"
        android:textAlignment="textStart"
        android:textColor="#5A30F2"
        android:textSize="16dp"
        app:cornerRadius="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:strokeColor="#5A30F2"
        app:strokeWidth="2dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="292dp"
        android:layout_height="62dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="158dp"
        android:layout_marginEnd="51dp"
        android:backgroundTint="#FFFFFF"
        android:drawableRight="@drawable/rectangle_51"
        android:fontFamily="@font/manrope_extrabold"
        android:onClick="selected"
        android:paddingLeft="20dp"
        android:text="АНГЛИЙСКИЙ"
        android:textAlignment="textStart"
        android:textColor="#5A30F2"
        android:textSize="16dp"
        app:cornerRadius="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:strokeColor="#5A30F2"
        app:strokeWidth="2dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="292dp"
        android:layout_height="62dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="548dp"
        android:layout_marginEnd="51dp"
        android:backgroundTint="#FFFFFF"
        android:drawableRight="@drawable/rectangle_8"
        android:fontFamily="@font/manrope_extrabold"
        android:onClick="showNotAvailableDialog"
        android:paddingLeft="20dp"
        android:text="ПОЛЬСКИЙ"
        android:textAlignment="textStart"
        android:textColor="#5A30F2"
        android:textSize="16dp"
        app:cornerRadius="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:strokeColor="#5A30F2"
        app:strokeWidth="2dp" />

    <Button
        android:id="@+id/button8"
        android:layout_width="345dp"
        android:layout_height="52dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="653dp"
        android:layout_marginEnd="28dp"
        android:backgroundTint="#5A30F2"
        android:fontFamily="@font/manrope"
        android:onClick="go_main"
        android:paddingLeft="20dp"
        android:text="ГОТОВО"
        android:textAlignment="center"
        android:textSize="20dp"
        app:cornerRadius="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="17dp"
        android:layout_marginTop="17dp"
        android:backgroundTint="#FFFFFF"
        android:contentDescription="arrow_back_language_choice"
        android:onClick="go_main"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ion_arrow_back_outline" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="92dp"
        android:layout_marginTop="85dp"
        android:layout_marginEnd="92dp"
        android:fontFamily="@font/manrope_bold"
        android:text="ВЫБЕРИТЕ ЯЗЫК"
        android:textColor="#000000"
        android:textSize="23dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>