<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="#EAEAEA"
    tools:context=".mops_spelling_task">

    <ImageButton
        android:id="@+id/imageButton14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="17dp"
        android:layout_marginTop="17dp"
        android:backgroundTint="#FEFAFD"
        android:contentDescription="arrow_back_language_choice"
        android:onClick="go_main"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ion_arrow_back_outline" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="138dp"
        android:layout_marginTop="85dp"
        android:layout_marginEnd="144dp"
        android:fontFamily="@font/manrope"
        android:text="ПРАВОПИСАНИЕ"
        android:textColor="#1820DF"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="49dp"
        android:layout_marginTop="116dp"
        android:layout_marginEnd="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/mops" />

    <Button
        android:id="@+id/button11"
        android:layout_width="171dp"
        android:layout_height="96dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="469dp"
        android:layout_marginEnd="206dp"
        android:backgroundTint="#FEFAFD"
        android:fontFamily="@font/manrope_extrabold"
        android:onClick="correct_mops"
        android:text="DOG"
        android:textColor="#1820DF"
        android:textSize="19dp"
        app:cornerRadius="17dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:strokeColor="#1820DF"
        app:strokeWidth="2dp" />

    <Button
        android:id="@+id/button18"
        android:layout_width="171dp"
        android:layout_height="96dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="589dp"
        android:layout_marginEnd="206dp"
        android:backgroundTint="#FEFAFD"
        android:fontFamily="@font/manrope_extrabold"
        android:onClick="incorrect_mops_black"
        android:text="BLACK"
        android:textColor="#1820DF"
        android:textSize="19dp"
        app:cornerRadius="17dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:strokeColor="#1820DF"
        app:strokeWidth="2dp" />

    <Button
        android:id="@+id/button19"
        android:layout_width="171dp"
        android:layout_height="96dp"
        android:layout_marginStart="207dp"
        android:layout_marginTop="589dp"
        android:layout_marginEnd="15dp"
        android:backgroundTint="#FEFAFD"
        android:fontFamily="@font/manrope_extrabold"
        android:onClick="incorrect_mops_dof"
        android:text="DOF"
        android:textColor="#1820DF"
        android:textSize="19dp"
        app:cornerRadius="17dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:strokeColor="#1820DF"
        app:strokeWidth="2dp" />

    <Button
        android:id="@+id/button17"
        android:layout_width="171dp"
        android:layout_height="96dp"
        android:layout_marginStart="207dp"
        android:layout_marginTop="469dp"
        android:layout_marginEnd="15dp"
        android:backgroundTint="#FEFAFD"
        android:fontFamily="@font/manrope_extrabold"
        android:onClick="incorrect_mops_doc"
        android:text="DOC"
        android:textColor="#1820DF"
        android:textSize="19dp"
        app:cornerRadius="17dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:strokeColor="#1820DF"
        app:strokeWidth="2dp" />

</androidx.constraintlayout.widget.ConstraintLayout>